/* pages/list/list.wxss */
page{
  background: #eeeeee;
}
.left{
  float: left;
  width: 30%;
}
.right{
  float: right;
  width: 70%;
}
.left>view{
  width: 100%;
  height: 44px;
  text-align: center;
  line-height: 40px;
  font-size: 16px;
  border-bottom: 1px solid #e5e5e5;
  border-right: 1px solid #e5e5e5;
  color: black;
  background: white;
}
.white{
  background: transparent !important;
  color: red !important;
}
.box{
  float: right;
  width: 95%;
  height: 160px;
  background: white;
  margin-top: 10px;
}
.box_bot{
  float: right;
  width: 95%;
  height: 350px;
  background: white;
  margin-top: 10px;
}
.img{
  display: block;
  width: 50%;
  height: 80px;
  margin: 20px auto;
}
.text{
  text-align: center;
  font-size: 20px;
}